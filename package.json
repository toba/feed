{
   "name": "@toba/feed",
   "version": "3.1.1",
   "description": "Toba syndication module",
   "private": false,
   "author": {
      "name": "Jason Abbott",
      "url": "http://github.com/toba"
   },
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "main": "cjs/index.js",
   "module": "esm/index.js",
   "types": "esm/index.d.ts",
   "files": [
      "esm",
      "cjs"
   ],
   "scripts": {
      "build:esm": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.esm.json",
      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "keywords": [
      "rss",
      "atom",
      "feed",
      "syndication",
      "xml",
      "wrapper",
      "blog"
   ],
   "devDependencies": {
      "@toba/develop": "^6.7.3",
      "@toba/test": "^4.1.13",
      "typescript": "^3.6.4"
   },
   "dependencies": {
      "@toba/tools": "^6.7.7"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/feed"
   },
   "bugs": {
      "url": "https://github.com/toba/feed/issues"
   }
}
